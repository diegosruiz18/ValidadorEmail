{
  "hosting": {
    "public": "dist/email-validator",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "headers": [ {
      "source": "**/*",
      "headers": [ {
        "key": "X-Frame-Options",
        "value": "SAMEORIGIN"
      } ]
    }, {
      "source": "**/*.@(jpg|jpeg|gif|png)",
      "headers": [ {
        "key": "Cache-Control",
        "value": "max-age=7200"
      } ]
    }, {
      "source": "**/*",
      "headers": [ {
        "key": "X-Content-Type-Options",
        "value": "nosniff"
      } ]
    } ]
  }
}
